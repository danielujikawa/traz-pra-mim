<div class= "Home">
  <div class="banner">
    <div class="banner-content">
      <h1>A faster and cheaper delivery</h1>
      <p>Search as a sender</p>
      <!--Home page form test 3 -->
      <form action="#" class="form-inline">
        <%= simple_form_for [@user, @route] do |f| %>

        <div class="form-group">
          <%= f.label :type %>
          <%= f.text_field :type, class: 'form-control' %>
        </div>

        <div class="form-group">
          <%= f.label :origin %>
          <%= f.text_field :origin, class: 'form-control' %>
        </div>

        <div class="form-group">
          <%= f.label :destination %>
          <%= f.text_field :destination, class: 'form-control' %>
        </div>

        <div class="form-group">
          <%= f.label :end_date_time %>
          <%= f.text_field :end_date_time, class: 'form-control' %>
        </div>

        <%= f.submit class: 'btn btn-primary' %>
        <% end %>
      </form>
    </div>
  </div>
</div>
